{
  "build": {
    "content": 
     [
      {
        "files": ["breadcrumb/toc.yml"]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],	
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "searchScop": [
         "Infrastructure Health"
       ],
      "breadcrumb_path": "/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "azure-monitor-health-knowledge",
    "markdownEngineName": "markdig"
  }
}
